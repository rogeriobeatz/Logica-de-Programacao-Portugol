programa {
  funcao inicio() {
    inteiro nota1, nota2, nota3
    real media

    escreva("primeira nota ")
    leia(nota1)

    escreva("segunda nota ")
    leia(nota2)

    escreva("última nota ")
    leia(nota3)

    media = (nota1 + nota2 + nota3)/3
    escreva("Sua média é " + media)

    se(media >= 70){
      escreva("média acima de 70")
    }senao{
      escreva("\n R E P R O V A D O - S E U - B U R R O")
    }
  }
}
